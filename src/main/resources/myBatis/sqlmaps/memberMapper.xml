<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Mem">
  	
  	<select id="loginCheck" resultType="com.spring.member.vo.MemberVo">
        SELECT M_ID FROM MEM
      		WHERE M_ID = 'TEST01' AND M_PW = '1234'
    </select>
   
	    <select id="viewMember" resultType="com.spring.member.vo.MemberVo">
	        SELECT 
	        	M_IDX,
	  	        M_ID,
				M_NAME,
				M_PW,
				TEL,
				EMAIL,
				LVL
	        FROM MEM
	        WHERE M_ID = #{m_id} AND M_PW = #{m_pw}
	    </select>
   
</mapper>        

